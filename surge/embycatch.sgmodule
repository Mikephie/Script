#!name=Emby观看保号✅
#!desc=Capture Emby playback events (start, progress, stop) over HTTP and HTTPS
#!author=🅜ⓘ🅚ⓔ🅟ⓗ🅘ⓔ
#!category=🔐APP
#!icon=https://raw.githubusercontent.com/deezertidal/private/main/icons/emby.png


[Script]
# Viclub服务器
viclub = type=http-request,pattern=^https?:\/\/su\.viclub\.top:8090\/emby\/videos\/[^\/]+\/(original|stream),requires-body=1,max-size=0,script-path=https://raw.githubusercontent.com/Mikephie/Script/main/qx/embytask.js

# 探花服务器
tanhua = type=http-request,pattern=^https?:\/\/tanhuatv\.site:443\/emby\/videos\/[^\/]+\/(original|stream),requires-body=1,max-size=0,script-path=https://raw.githubusercontent.com/Mikephie/Script/main/qx/embytask.js

# Nanflix服务器
nanflix = type=http-request,pattern=^https?:\/\/us\.nanflix\.net:2231\/emby\/videos\/[^\/]+\/(original|stream),requires-body=1,max-size=0,script-path=https://raw.githubusercontent.com/Mikephie/Script/main/qx/embytask.js

# 惊蛰服务器
jingzhe = type=http-request,pattern=^https?:\/\/emby\.jingzhe\.pro:80\/emby\/videos\/[^\/]+\/(original|stream),requires-body=1,max-size=0,script-path=https://raw.githubusercontent.com/Mikephie/Script/main/qx/embytask.js

# MisakaF服务器
moe = type=http-request,pattern=^https?:\/\/newplay\.emby\.moe\/stream\?md5=.+,requires-body=1,max-size=0,script-path=https://raw.githubusercontent.com/Mikephie/Script/main/qx/embytask.js

# 定时任务
emby_task = type=cron,cronexp="30 20 7,14,21,28 * *",wake-system=1,timeout=60,script-path=https://raw.githubusercontent.com/Mikephie/Script/main/qx/embytask.js

[MITM]
hostname = %APPEND% su.viclub.top, tanhuatv.site, us.nanflix.net, emby.jingzhe.pro, newplay.emby.moe