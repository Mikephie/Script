[Argument]
appName = input,"微商海报大师",tag=app的名字,desc=填写app的用户名，用于提交app的相关信息
appCategory = select,"✅签到","🚫广告","🔐APP","🛠️工具",tag=app的分类
isSupportChinese = switch,true,tag=是否支持中文,desc=选择app是否支持中文

[Script]
http-response ^https?:\/\/api-poster\.ycase\.cn\/api\/user\/info script-path=https://raw.githubusercontent.com/Mikephie/Script/main/qx/wshb1.js, requires-body=true, timeout=60, tag=wshb1, argument=[{appName},{appCategory},{isSupportChinese}],enable = {cookieScriptEnable}
http-response ^https?:\/\/api-poster\.ycase\.cn\/api\/User\/home script-path=https://raw.githubusercontent.com/Mikephie/Script/main/qx/wshb2.js, requires-body=true, timeout=60, tag=wshb2


[MITM]
hostname = api-poster.ycase.cn