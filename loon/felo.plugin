#!name = Felo实时翻译🔐APP解锁
#!desc = 工作效率 - 插件
#!author = 🅜ⓘ🅚ⓔ🅟ⓗ🅘ⓔ
#!icon = https://raw.githubusercontent.com/Mikephie/icons/main/icon/felo.png

[Argument]
appName = input,"Felo实时翻译",tag=app的名字,desc=填写app的用户名，用于提交app的相关信息
appCategory = select,"🔐APP","✅签到","🚫广告","🛠️工具",tag=app的分类
isSupportChinese = switch,true,tag=是否支持中文,desc=选择app是否支持中文

[Script]
http-response ^https?:\/\/(translator|accounts).felo.me\/api\/user.*$ script-path=https://raw.githubusercontent.com/Mikephie/Script/main/qx/felosubs.js, requires-body=true, timeout=60, tag=felo, argument=[{appName},{appCategory},{isSupportChinese}],enable = {cookieScriptEnable}
http-request ^https?:\/\/translator.felo.me\/api\/translation\/thirdToken\?token_channel script-path=https://raw.githubusercontent.com/Mikephie/Script/main/qx/felotk.js, requires-body=true, timeout=60, tag=felotk


[MITM]
hostname = accounts.felo.me,translator.felo.me