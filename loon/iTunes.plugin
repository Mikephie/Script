#!name = 🔐APP解锁🛠️工具🚫去广告✅签到
#!desc = FILL - 插件
#!author = 🅜ⓘ🅚ⓔ🅟ⓗ🅘ⓔ
#!icon = https://raw.githubusercontent.com/Loon0x00/Loon0x00.github.io/main/static/img/loon.png

[Argument]
appName = input,"FILL",tag=app的名字,desc=填写app的用户名，用于提交app的相关信息
appCategory = select,"🔐APP","✅签到","🚫广告","🛠️工具",tag=app的分类
isSupportChinese = switch,true,tag=是否支持中文,desc=选择app是否支持中文
cron = input,"0 10 * * *",tag=定时任务时间,desc=定时任务的cron表达式
cookieScriptEnable = switch,true,tag=是否启用获取cookie的脚本

[Script]
http-request FILL script-path=FILL, tag=FILL, argument=[{appName},{appCategory},{isSupportChinese}],enable = {cookieScriptEnable}

cron {cron} script-path=FILL, tag= FILL, enable=true

[MITM]
hostname = FILL