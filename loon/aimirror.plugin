#!name = AIMirror🔐APP
#!desc = 图像制作 - 插件
#!author = 🅜ⓘ🅚ⓔ🅟ⓗ🅘ⓔ
#!icon = https://raw.githubusercontent.com/Mikephie/icons/main/icon/aimirror.png

[Argument]
appName = input,"AIMirror",tag=app的名字,desc=填写app的用户名，用于提交app的相关信息
appCategory = select,"✅签到","🚫广告","🔐APP","🛠️工具",tag=app的分类
isSupportChinese = switch,true,tag=是否支持中文,desc=选择app是否支持中文

[Script]
http-response ^(https:\/\/be\.aimirror\.fun\/(draw|draw\/process|query_is_vip|users\/video_render_count|query_consumable_quota|users\/discount)) script-path=https://raw.githubusercontent.com/Mikephie/Script/main/qx/aimirror.js, requires-body=true, timeout=60, tag=aimirror, argument=[{appName},{appCategory},{isSupportChinese}],enable = {cookieScriptEnable}


[MITM]
hostname = be.aimirror.fun