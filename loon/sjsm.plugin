[Argument]
appName = input,"ToonMe",tag=app的名字,desc=填写app的用户名，用于提交app的相关信息
appCategory = select,"✅签到","🚫广告","🔐APP","🛠️工具",tag=app的分类
isSupportChinese = switch,true,tag=是否支持中文,desc=选择app是否支持中文

[Script]
http-response ^https?:\/\/purchases\.ws\.pho\.to\/api\/v2\/apple\/verifyReceipt script-path=https://raw.githubusercontent.com/Guding88/Script/main/ToonMe.js, requires-body=true, timeout=60, tag=ToonMe, argument=[{appName},{appCategory},{isSupportChinese}],enable = {cookieScriptEnable}


[MITM]
hostname = purchases.ws.pho.to