[Argument]
appName = input,"试卷扫描",tag=app的名字,desc=填写app的用户名，用于提交app的相关信息
appCategory = select,"✅签到","🚫广告","🔐APP","🛠️工具",tag=app的分类
isSupportChinese = switch,true,tag=是否支持中文,desc=选择app是否支持中文

[Script]
http-response ^https?:\/\/appss.baomingding.com\/\/app\/account\/getAccountInfo script-path=https://raw.githubusercontent.com/Mikephie/Script/main/qx/sjsm.js, requires-body=true, timeout=60, tag=sjsm, argument=[{appName},{appCategory},{isSupportChinese}],enable = {cookieScriptEnable}


[MITM]
hostname = appss.baomingding.com

# 试卷扫描-拍照清除笔迹，还原空白试卷，错题标记重组（永久会员）
# hostname = appss.baomingding.com
http-response ^https?:\/\/appss.baomingding.com\/\/app\/account\/getAccountInfo script-path=https://raw.githubusercontent.com/Mikephie/Script/main/qx/sjsm.js, requires-body=true, timeout=60, tag=sjsm
