#!name = Flightradar24🔐APP解锁
#!desc = 航班查询 - 插件
#!author = 🅜ⓘ🅚ⓔ🅟ⓗ🅘ⓔ
#!icon = https://raw.githubusercontent.com/Mikephie/icons/main/loon/flightradar.png

[Argument]
appName = input,"Flightradar24",tag=app的名字,desc=填写app的用户名，用于提交app的相关信息
appCategory = select,"✅签到","🚫广告","🔐APP","🛠️工具",tag=app的分类
isSupportChinese = switch,true,tag=是否支持中文,desc=选择app是否支持中文

[Script]
http-response ^https://mobile.flightradar24.com/mobile/(user-session|subscribe) script-path=https://raw.githubusercontent.com/Mikephie/Script/main/qx/flightradar24.js, requires-body=true, timeout=60, tag=Flightradar24, argument=[{appName},{appCategory},{isSupportChinese}],enable = {cookieScriptEnable}


[MITM]
hostname = mobile.flightradar24.com