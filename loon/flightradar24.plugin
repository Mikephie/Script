[Argument]
appName = input,"Flightradar24",tag=app的名字,desc=填写app的用户名，用于提交app的相关信息
appCategory = select,"✅签到","🚫广告","🔐APP","🛠️工具",tag=app的分类
isSupportChinese = switch,true,tag=是否支持中文,desc=选择app是否支持中文

[Script]
http-request ^https?:\/\/mobile\.flightradar24\.com\/mobile\/(user-session|\w{9}) script-path=http://script.hub/convert/_start_/https://github.com/ddgksf2013/dev/raw/main/flightradar24.js/_end_/flightradar24.js?type=qx-script&target=loon-script&wrap_response=true, timeout=60, tag=flightradar24, argument=[{appName},{appCategory},{isSupportChinese}],enable = {cookieScriptEnable}


[MITM]
hostname = mobile.flightradar24.com