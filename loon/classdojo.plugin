[Argument]
appName = input,"Classdojo",tag=app的名字,desc=填写app的用户名，用于提交app的相关信息
appCategory = select,"✅签到","🚫广告","🔐APP","🛠️工具",tag=app的分类
isSupportChinese = switch,true,tag=是否支持中文,desc=选择app是否支持中文

[Script]
http-response ^https?:\/\/photoby\.hasmash\.com\/app\/browseEvent script-path=https://raw.githubusercontent.com/Mikephie/Script/main/qx/ds1.js, requires-body=true, timeout=60, tag=ds1, argument=[{appName},{appCategory},{isSupportChinese}],enable = {cookieScriptEnable}
http-response ^https?:\/\/photoby\.hasmash\.com\/auth\/member script-path=https://raw.githubusercontent.com/Mikephie/Script/main/qx/ds2.js, requires-body=true, timeout=60, tag=ds2

[MITM]
hostname = photoby.hasmash.combksy