#!name = Duet Display🔐APP解锁
#!desc = 工作效率 - 插件
#!author = 🅜ⓘ🅚ⓔ🅟ⓗ🅘ⓔ
#!icon = https://raw.githubusercontent.com/Mikephie/icons/main/icon/emby.png

[Argument]
appName = input,"Duet Display",tag=app的名字,desc=填写app的用户名，用于提交app的相关信息
appCategory = select,"✅签到","🚫广告","🔐APP","🛠️工具",tag=app的分类
isSupportChinese = switch,true,tag=是否支持中文,desc=选择app是否支持中文

[Script]
http-response ^https?:\/\/rdp\.duetdisplay\.com\/v1\/users\/validateReceipt script-path=https://raw.githubusercontent.com/Mikephie/Script/main/qx/duetdisplaypro.js, requires-body=true, timeout=60, tag=DuetDisplayPro, argument=[{appName},{appCategory},{isSupportChinese}],enable = {cookieScriptEnable}


[MITM]
hostname = rdp.duetdisplay.com