[Script]
http-response ^https:\/\/api\.revenuecat\.com\/.+\/(receipts$|subscribers\/?(.*?)*$) script-path= https://raw.githubusercontent.com/Mikephie/Script/main/qx/RevenueCat.js, requires-body=true, tag=RevenueCat, argument={argument}

http-request ^https:\/\/api\.revenuecat\.com\/.+\/(receipts$|subscribers\/?(.*?)*$) script-path= https://raw.githubusercontent.com/Mikephie/Script/main/qx/RevenueCat.js, requires-body=false, tag=RevenueCat

[MITM]
hostname = api.revenuecat.com

[Argument]
RevenueCatBodyEnable = switch,true,tag=是否启用Body脚本
RevenueCatHeaderEnable = switch,true,tag=是否启用Header脚本
argument = input, desc=pro:sub